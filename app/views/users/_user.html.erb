<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Emergency contact:</strong>
    <%= user.emergency_contact %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= user.birthday %>
  </p>

  <p>
    <strong>Injury:</strong>
    <%= user.injury %>
  </p>

  <p>
    <strong>Photo permission:</strong>
    <%= user.photo_permission %>
  </p>

</div>
